{
  "name": "crowdmeter-server",
  "version": "1.0.0",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "nodemon server/index.js",
    "seed": "curl -X POST http://localhost:5175/api/cafes/seed",
    "refresh": "curl -X POST http://localhost:5175/api/cafes/refresh",
    "dev:auto": "node dev-auto.js",
    "dev:setup": "./dev-setup.sh",
    "setup": "npm run seed && sleep 2 && npm run refresh",
    "refresh:loop": "while true; do sleep 420; curl -X POST http://localhost:5175/api/cafes/refresh; echo ''; done"
  },
  "dependencies": {
    "@googlemaps/google-maps-services-js": "^3.3.35",
    "axios": "^1.7.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.0",
    "express": "^4.19.2",
    "mongoose": "^8.3.1",
    "node-fetch": "^3.3.2",
    "puppeteer": "^22.15.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.0"
  }
}
